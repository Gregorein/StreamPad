{
  "applicationId": "gregorein.streampadd",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 8080,
  "enableServer": true,
  "enableNativeAPI": true,
  "singlePageServe": true,
  "tokenSecurity": "one-time",  
  "documentRoot": "/build/",
  "url": "/",
  "nativeAllowList": [
    "app.*",
    "fileSystem.*"
  ],  
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "modes": {
    "window": {
      "title": "StreamPad",
      "width": 1280,
      "height": 800,
      "center": true,
      "icon": "/src/assets/icon.png",
      "resizable": false,
      "hidden": false,

      "borderless": true,
      "enableInspector": true
    }
  },
  "cli": {
    "binaryName": "streampad",
    "resourcesPath": "/build/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "5.3.0",
    "clientVersion": "5.3.0",
    "frontedLibrary": {
      "patchFile": "/src/index.html",
      "devUrl": "http://localhost:3000/"
    }
  },
  "distributionPath": "/build/"
}